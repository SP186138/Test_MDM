,STRET_NAME=B.STRET_NAME
,APT_NUM=B.APT_NUM
,PO_BOX_NUM=B.PO_BOX_NUM
,CITY_NAME=B.CITY_NAME
,POSTL_AREA_CODE=B.POSTL_AREA_CODE
,LAT_MEAS=B.LAT_MEAS
,LONG_MEAS=B.LONG_MEAS
,TERR_CODE=B.TERR_NAME
,CNTRY_CODE=B.CNTRY_NAME,
SYS_SOURCE=B.SYS_SOURCE ,
