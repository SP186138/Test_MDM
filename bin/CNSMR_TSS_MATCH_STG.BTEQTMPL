AND MKTNG_PGM_NBR = 2 AND PA.CNTRY_CODE = 'JPN'
AND CAST(PA.SYS_SOURCE AS INTEGER) IN (SEL LOAD_ID FROM MDM_LOAD_CONTROL);
.IF ERRORLEVEL > 0 THEN .GOTO EXIT;

