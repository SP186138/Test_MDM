  ON B.MKTNG_PGM_NBR = C.MKTNG_PGM_NBR
 AND B.LEGAL_ENT_NBR = C.LEGAL_ENT_NBR
 AND B.REGIS_PRSNA_ID = C.REGIS_PRSNA_ID 
