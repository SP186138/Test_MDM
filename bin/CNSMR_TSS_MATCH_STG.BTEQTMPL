AND a.CNTRY_CODE = 'JPN' and REGIS_PRSNA_POSTL_ADDR.CNTRY_CODE = 'JPN'
AND CAST(REGIS_PRSNA_POSTL_ADDR.SYS_SOURCE AS INTEGER) IN (SEL LOAD_ID FROM MDM_LOAD_CONTROL);
