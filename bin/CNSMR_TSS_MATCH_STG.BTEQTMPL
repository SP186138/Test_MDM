 ,DR_MIDDLE_NAME
 ,DR_LAST_NAME
 ,DR_MRMRS
 ,DR_NAME_GENER
 ,DR_NAME_SUFFIX
 ,DR_NAME_GENDER
 ,DR_COUNTRY_NAME
 ,DR_REGION_NAME
 ,DR_CITY_NAME
 ,DR_POSTAL_CODE
 ,DR_STREET_NAME
 ,DR_HOUSE_NUMBER1
 ,DR_HOUSE_NUMBER2
 ,DR_POBOX_NUMBER
 ,DR_ADDRESS
 ,DR_ADDRESS2
 ,DR_ADDRESS3
 ,CLEANSED_PHONE_1
 ,CLEANSED_EMAIL_1
 ,PR_REV_GROUP
 ,GOUT_MATCH_LEVEL
 ,WINDOW_KEY_01
 ,REFERENCE_HOUSEHOLD_ID
 ,REFERENCE_LEGALENTITYKEY
