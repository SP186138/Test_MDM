INSERT INTO MDM.LOAD_INFO_ARCHIVE
SELECT * FROM MDM.LOAD_INFO
WHERE CAST(SYS_CREATION_DATE AS DATE) <=(SELECT DATE - INTERVAL '2' MONTH);

.IF ERRORLEVEL > 0 THEN .GOTO EXIT;

DELETE FROM MDM.LOAD_INFO 
WHERE CAST(SYS_CREATION_DATE AS DATE) <=(SELECT DATE - INTERVAL '2' MONTH);

