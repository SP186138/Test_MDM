  <DEFINE_METHOD Name="pngSETPreviousLoadStatus">
    <!--**********************************************************************************************************
*   Module Name	: pngSETPreviousLoadStatus
*		Created Date	: Oct 27, 2011
*		Created By   	: Kalyana Chakravarthy
*		Called Functions: 
*		Called By Functions:  pngSETPreviousLoadStatus bat files
*		Short Desc	: This API sets the COuntry Specific PreLoad failure global variable to success 
*  		Change Log 		:
*		DATE							LC						OWNER		DESCRIPTION
*
**********************************************************************************************************-->
    <RULE>
      <ACTION>
        <SET Var="{concat('previousLoadStatus',$thisParam/CNTRY_NAME/@Value)}" FromValue="Success" Scope="Global"/>
      </ACTION>
    </RULE>
  </DEFINE_METHOD>
