    </RULE>
  </DEFINE_METHOD>
  <DEFINE_METHOD Name="pngSETCPUProcessorTimeOnServiceStartup">
    <!--**********************************************************************************************************
*   Module Name	: pngSETPreviousLoadStatus
*		Created Date	: Oct 27, 2011
*		Created By   	: Kalyana Chakravarthy
*		Called Functions:
*		Called By Functions:  
*		Short Desc	: This API sets the CPU Processor Percent Time on Service Startup
*  		Change Log 		:
*		DATE							LC						OWNER		DESCRIPTION
*
**********************************************************************************************************-->
    <RULE>
      <ACTION>
        <SET Var="xmlfilepath" FromValue="\custom\pngrelease3\bin\pngSETCPUPercentProcessorTime.xml"/>
        <LOAD_XML_FILE FromValue="{concat($MDM_Install_Dir,$xmlfilepath)}" Var="resCPUTime"/>
        <SET Var="CPUPercentProcessorTime" FromValue="{$resCPUTime/REQUEST/CPUPercent/@Value}" Scope="Global"/>
      </ACTION>
    </RULE>
  </DEFINE_METHOD>
