            </APPEND_TO_XML>
          </THEN>
          <ELSE/>
        </IF_TEST>
        <ADD_DOC_SMART ServiceName="BCM_MASTER" Name="CNTNT_ELEM" AssignToVar="createContentIncresult">
          <CNTNT_ELEM_ID Value="{$thisParam/NEW_CNTNT_ELEM_ID/@Value}"/>
          <MKTNG_PGM_NBR Value="{$thisParam/MKTNG_PGM_NBR/@Value}"/>
          <CNTNT_NAME Value="{$thisParam/INCTV_NAME/@Value}"/>
          <INCTV_NBR Value="{$thisParam/NEW_INCTV_NBR/@Value}"/>
          <CNTNT_ELEM_TYPE_CODE Value="CC"/>
          <TO_XML SelectList="$startDate/*"/>
          <TO_XML SelectList="$endDate/*"/>
        </ADD_DOC_SMART>
      </ACTION>
    </RULE>
  </DEFINE_METHOD>
  <DEFINE_METHOD Name="getDataSrcNbrInctv">
    <RULE>
      <ACTION>
        <GET_DOC_SMART Name="DATA_SRCE" AssignToVar="getDataSrceNbr">
          <SELECT>
            <DATA_SRCE_NAME/>
          </SELECT>
        </GET_DOC_SMART>
        <CLONE_XML DocVar="getDataSrceNbr" AssignToVar="thisReturn"/>
      </ACTION>
    </RULE>
  </DEFINE_METHOD>
  <DEFINE_METHOD Name="getExtrnlCpnDataSrce">
    <RULE>
      <ACTION>
        <TO_DOCVAR AssignToVar="allDataSrc">
          <ROOT>
            <EXTRNL_DATA_SRCE_TYPE/>
          </ROOT>
        </TO_DOCVAR>
        <EXECUTE_SQL_QUERY Value="SEL A.DATA_SRCE_NBR,DATA_SRCE_NAME FROM DATA_SRCE A INNER JOIN DATA_SRCE_MKTNG_PGM B ON A. DATA_SRCE_NBR=B.DATA_SRCE_NBR         WHERE MKTNG_PGM_NBR= {$thisParam/MKTNG_PGM_NBR/@Value} ORDER BY DATA_SRCE_NAME ASC;" AssignToVar="ExtrnDataSrcValues"/>
        <FOR_EACH SelectList="$ExtrnDataSrcValues/SQL_RESULT" CurrentElement="currentElement">
          <SET Var="extrName1" FromValue="{$currentElement/DATA_SRCE_NAME/@Value}"/>
          <SET Var="extrNbr" FromValue="{$currentElement/DATA_SRCE_NBR/@Value}"/>
          <SET Var="extrNbr1" FromValue="{concat($extrNbr,'~')}"/>
          <SET Var="extrName" FromValue="{concat($extrNbr1,$extrName1)}"/>
          <APPEND_TO_XML SelectList="$allDataSrc/EXTRNL_DATA_SRCE_TYPE">
            <OPTION Id="{$extrNbr}" Value="{$extrName}"/>
          </APPEND_TO_XML>
        </FOR_EACH>
        <CLONE_XML DocVar="allDataSrc" AssignToVar="thisReturn"/>
      </ACTION>
    </RULE>
  </DEFINE_METHOD>
</DOCUMENTS>
