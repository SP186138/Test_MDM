        </TO_DOCVAR>
        <!--<EXECUTE_SQL_QUERY Value="SEL DISTINCT A.PROD_FAMLY_ID ,  A.PROD_ITEM_UPC,C.PROD_FAMLY_NAME FROM PROD_ITEM_HIER_ASSOC A LEFT OUTER JOIN INCTV_ITEM  B ON A.PROD_ITEM_UPC=B.PROD_ITEM_UPC LEFT OUTER JOIN PROD_FAMLY C ON A.PROD_FAMLY_ID=C.PROD_FAMLY_ID WHERE    A.PROD_ITEM_UPC='{$thisParam/PROD_ITEM_UPC/@Value}'" AssignToVar="allProdFamilyTypes"/>-->
