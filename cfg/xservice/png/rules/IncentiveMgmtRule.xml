            <EXECUTE_SQL_QUERY Value="SELECT brand_name FROM BRAND where brand_code='{$thisParam/BRAND_CODE/@Value}';" AssignToVar="brandItemName"/>
            <SET Var="V_INCTV_ITEM_NAME" FromValue="{$brandItemName/SQL_RESULT/BRAND_NAME/@Value}"/>
            <ADD_DOC_SMART ServiceName="BCM_MASTER" Name="INCTV_ITEM" AssignToVar="addInctvItem">
