  <DEFINE_METHOD Name="createInctvItem">
    <RULE>
      <ACTION>
        <IF_TEST Test="$thisParam/ASSIGNED_ITEMS/ASSIGNED_ITEM = null or $thisParam/ASSIGNED_ITEMS/ASSIGNED_ITEM =''">
