              <CREATE_DOCUMENT_ID Name="INCTV_ITEM_ID" AssignToVar="inctvItemId1"/>
              <SET Var="selectedIncentiveItemID1" FromValue="{$inctvItemId1}"/>
              <EXECUTE_SQL_QUERY Value="SELECT PROD_ITEM_NAME FROM PROD_ITEM WHERE PROD_ITEM_UPC='{$currElement/@Value}';" AssignToVar="ProdItemName"/>
