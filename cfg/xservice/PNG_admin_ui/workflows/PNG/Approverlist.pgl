var xpath = &quot;/RESPONSES/RESPONSE/LOGIN_NAME&quot;;
SendAjaxRequest(&quot;ADMIN_UI&quot;,request,'getUserCallbackForAllAction',xpath);
}
else{

var request= &quot;&lt;REQUEST Name='getMdmAllUserGrp' ServiceName='ADMIN_UI'>&lt;/REQUEST>&quot;;
var xpath = &quot;/RESPONSES/RESPONSE/GRP_NAME&quot;;
SendAjaxRequest(&quot;ADMIN_UI&quot;,request,'getGrpCallbackForAllAction',xpath);
}
}
function getUserCallbackForAllAction(node)
{
