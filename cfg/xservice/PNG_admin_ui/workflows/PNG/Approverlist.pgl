targetSubCategory.add(opt,targetSubCategory[1]);
var opt = document.createElement(&quot;option&quot;);
opt.text = &quot;UPDATE&quot;;
targetSubCategory.add(opt,targetSubCategory[2]);
var opt = document.createElement(&quot;option&quot;);
opt.text = &quot;DELETE&quot;;
targetSubCategory.add(opt,targetSubCategory[3]);
var opt = document.createElement(&quot;option&quot;);
opt.text = &quot;ALL&quot;;
targetSubCategory.add(opt,targetSubCategory[4]);
$('#ACTION_TYPE').val(&quot;Select...&quot;).trigger(&quot;change&quot;);
    }

