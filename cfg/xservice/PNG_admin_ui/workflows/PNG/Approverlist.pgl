}

function getGrpCallbackForAllAction(node)
{
document.form.Assigned_Approvers.options.length = 0;
document.form.Available_Approvers.options.length = 0;
for (i=0;i&lt;node.length;i++)
{
document.form.Available_Approvers.options[i]=new Option(node[i].getAttribute(&quot;Value&quot;), node[i].getAttribute(&quot;Value&quot;));

}
sortDropDown('Available_Approvers');
}
function getApprovalsForAllAction(buttonId)
{
button_Id=buttonId;
if(document.form.EVENT_NAME.value != &quot;&quot;)
{
var request=&quot;&lt;GET_DOCUMENT Name='Approval_Groups' ServiceName='APPROVAL' DirtyRead='true'> &lt;EVENT_NAME Value='&quot;+document.form.EVENT_NAME.value+&quot;'/>&lt;IS_ACTIVE Value='True'/>&lt;/GET_DOCUMENT>&quot;;
var xpath = &quot;/RESPONSES/RESPONSE/Approval_Groups&quot;;
SendAjaxRequest(&quot;APPROVAL&quot;,request,'getApprovalsCallbackForAllAction',xpath);
