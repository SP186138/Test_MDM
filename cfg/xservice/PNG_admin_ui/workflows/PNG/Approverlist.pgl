function getApprovalsCallbackForAllAction(node)
{
var count;
//if(document.all)
	count = node.length; //for IE
//else
//	count = node.childNodes.length; // For FF
if(count > 0)
{
core_confirm_jquery('APPROVAL_DEL_CONFIRM','onDeleteCallback');
}
else
{
form.BUTTON_ID.value = button_Id;
form.submit();
grayOut(true);
}
}
 function onDeleteCallback(response)
{
	if( response == 'yes' )
	{
     form.BUTTON_ID.value = button_Id;
     form.submit();
     grayOut(true);
    }
}   
   
function getAllApprovals()
{
if(document.form.EVENT_NAME.value != &quot;&quot;)
{
if (document.form.ACTION_TYPE.value != &quot;&quot;)
{
var request=&quot;&lt;GET_DOCUMENT Name='Approval_Groups' ServiceName='APPROVAL' DirtyRead='true'>&lt;ACTION_TYPE Value='&quot;+document.form.ACTION_TYPE.value+&quot;'/> &lt;EVENT_NAME Value='&quot;+document.form.EVENT_NAME.value+&quot;'/>&lt;IS_ACTIVE Value='True'/>&lt;/GET_DOCUMENT>&quot;;
var xpath = &quot;/RESPONSES/RESPONSE/Approval_Groups&quot;;
SendAjaxRequest(&quot;APPROVAL&quot;,request,'getApprovalsCallback',xpath);
}
}
}
    
    
function getApprovalsCallback(node)
{
if(node.length > 0)
{
core_alert_jquery(&quot;Approval already exists on &quot;+ document.form.EVENT_NAME.value+&quot; for   &quot;+ document.form.ACTION_TYPE.value+&quot; action type.&quot;);
return;
}
else
return;
}
function hasOptions(obj)
{
if (obj!=null &amp;&amp; obj.options!=null)
{
return true;
}
return false;
}

function moveSelectedOptions(from,to) {
// Move them over
if(from==&quot;Available_Approvers&quot;){
from=document.form.Available_Approvers;
}else{
from=document.form.Assigned_Approvers;
}
if(to==&quot;Available_Approvers&quot;){
to=document.form.Available_Approvers;
}else{
to=document.form.Assigned_Approvers;
}

if (!hasOptions(from))
{
return;
}
for (var i=0; i&lt;from.options.length; i++)
{
var o = from.options[i];
if (o.selected)
{
if (!hasOptions(to))
{
var index = 0;
}
else
{
var index=to.options.length;
}
to.options[index] = new Option( o.text, o.value, true, false);
}
}
// Delete them from original
for (var i=(from.options.length-1); i>=0; i--)
{
var o = from.options[i];
if (o.selected)
{
from.options[i] = null;
}
}
from.selectedIndex = -1;
to.selectedIndex = -1;
}
		
function onSaveOrUpdate(obj,buttonId)
{
if(obj==&quot;Available_Approvers&quot;){
obj=document.form.Available_Approvers;
}else{
obj=document.form.Assigned_Approvers;
}

var bIsValid = true;
document.form.SUBMIT_AS_XML.value = 'false';

bIsValid =  (isValid_InnerStep1());
 
if( bIsValid == false )
{
validation_alert('InnerStep1');
}
for(h=0;h&lt;document.getElementById('Assigned_Approvers').options.length;h++){
document.getElementById('Assigned_Approvers').options[h].selected=true;
}
 
bIsValid1 = (isValid_InnerStep2());

if( bIsValid1 == false)
{
validation_alert('InnerStep2');
}
    
if ( bIsValid == true &amp;&amp; bIsValid1 == true)
{
if(document.form.NOTIFICATION_TYPE.value == '')
{
document.form.NOTIFICATION_TYPE.value=&quot;ALL&quot;;
}
if (obj.options.length == '0')
{
core_alert_jquery(&quot;ASSIGN_APPROVER&quot;);
return;
}
if(document.form.APPROVAL_OBJECT_TYPE.value=='Business Rules')
{
	document.form.EVENT_NAME.options.length = 0;
	document.form.EVENT_NAME.options[0] = new Option('Business Rules', 'Business Rules', true, true);
}
if (document.form.EVENT_NAME.value == '')
{
core_alert_jquery("Please select one "+document.form.APPROVAL_OBJECT_TYPE.value);
return;
}

for (var i=0; i&lt;obj.options.length; i++)
{
obj.options[i].selected = true;
}
if(document.form.NOTIFICATION_TYPE.value == '')
{
document.form.NOTIFICATION_TYPE.value=&quot;ALL&quot;;
}
if(document.form.ACTION_TYPE.value == 'ALL'){
getApprovalsForAllAction(buttonId);
return;
}
form.BUTTON_ID.value = buttonId;
form.submit();
grayOut(true);
}
}
    
