<DOCUMENTS>
		<DEFINE_METHOD Name="execute_snapshot">
		<RULE>
			<ACTION>
				 <SET Var="taskID" FromValue="{$thisParam/TASKID/@Value}"/>
				 <EXEC_PROC AssignToVar="results" Name="ON_REQUEST_SNAPSHOT">
    						<SNAPSHOT_ID Value="{$taskID}" Type="IN" DataType="int"/>
  				</EXEC_PROC>
			</ACTION>
		</RULE>
	</DEFINE_METHOD>
</DOCUMENTS>

