)

bteq < %FAILUREBTEQ1% >> %MDM_CUSTOM_DIR%\log\TSS_MATCH_OUTPUT_%CNTRY1%.LOG
set ERR1=%ERRORLEVEL%
IF NOT %ERR1% == 0 (
